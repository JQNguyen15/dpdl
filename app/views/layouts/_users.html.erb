<div class="col-md-4">
	<% @users = online_users %>
	<h1>Online Users</h1>
	<% if @users %>
	        <ul> 
	        <% @users.each do |user| %>
	            <li>
	              <%= user.nickname %> 
	              <%= user.mmr %>
	            </li>
	        <% end %>
	        </ul>
	<% end %>
</div>